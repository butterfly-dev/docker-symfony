<?xml version="1.0"?>
<ruleset name="Sample">
    <description>Eleven Labs coding standard</description>

    <rule ref="vendor/escapestudios/symfony2-coding-standard/Symfony/ruleset.xml">
        <exclude name="Symfony.Functions.ScopeOrder"/>
        <exclude name="Symfony.Commenting.FunctionComment"/>
        <exclude name="Symfony.Commenting.ClassComment.Missing"/>
        <exclude name="Symfony.NamingConventions.ValidClassName.InvalidTraitName"/>
        <exclude name="Symfony.NamingConventions.ValidClassName.InvalidExceptionName"/>
        <exclude name="Symfony.NamingConventions.ValidClassName.InvalidInterfaceName"/>
    </rule>
</ruleset>
